@app.get("/{circleguid}/{{variable_name}}_manager{{suffix}}/{{variable_name}}/{id}")
def {{variable_name}}_get(circleguid: int, id: str)-> {{type_name}}:
    return {{variable_name}}_get_internal{{suffix}}(circleguid, id)

