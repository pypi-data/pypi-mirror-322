      <field name="price_unit">100.0</field>
      <field name="invoice_id" ref="invoice_somconnexio.invoice_bi_demo" />
      <field name="name">Il·limitades 20GB</field>
      <field name="account_id" ref="invoice_somconnexio.account_demo" />
      <field name="invoice_line_tax_ids" eval="[(6, 0, [ref('l10n_es.1_account_tax_template_s_iva21s')])]"/>
   </record>
   <record id="invoice_oc_demo" model="account.invoice">
      <field name="partner_id" ref="somconnexio.res_partner_1_demo" />
      <field name="type">out_invoice</field>
      <field name="payment_mode_id" ref="somconnexio.payment_mode_inbound_sepa"/>
      <field name="payment_term_id" ref="account.account_payment_term_net"/>
      <field name="account_id" ref="invoice_somconnexio.account_demo" />
      <field name="journal_id" ref="somconnexio.consumption_invoices_journal" />
