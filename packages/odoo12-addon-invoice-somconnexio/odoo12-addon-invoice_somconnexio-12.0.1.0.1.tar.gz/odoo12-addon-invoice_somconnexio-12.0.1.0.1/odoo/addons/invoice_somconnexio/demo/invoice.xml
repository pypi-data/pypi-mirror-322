      <field name="amount_total">242.00</field>
      <field name="amount_tax">42.00</field>
      <field name="amount_untaxed">200.00</field>
      <field name="date_invoice" eval="datetime.now().date()  - timedelta(days=22)"/>
      <field name="move_id" ref="invoice_somconnexio.account_move_demo_oc" />
      <field name="state">paid</field>
   </record>
   <record id="invoice_oc_line_demo" model="account.invoice.line">
      <field name="product_id" ref="somconnexio.TrucadesIllimitades20GB" />
      <field name="quantity">1.0</field>
      <field name="price_unit">100.0</field>
      <field name="invoice_id" ref="invoice_somconnexio.invoice_oc_demo" />
      <field name="name">Il·limitades 20GB</field>
      <field name="account_id" ref="invoice_somconnexio.account_demo" />
      <field name="invoice_line_tax_ids" eval="[(6, 0, [ref('l10n_es.1_account_tax_template_s_iva21s')])]"/>
   </record>
