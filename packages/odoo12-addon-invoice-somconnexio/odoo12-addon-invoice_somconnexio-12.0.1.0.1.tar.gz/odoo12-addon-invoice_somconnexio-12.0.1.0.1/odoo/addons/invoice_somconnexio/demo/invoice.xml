      <field name="number">SO2024-0001</field>
      <field name="amount_total">121.00</field>
      <field name="amount_tax">21.00</field>
      <field name="amount_untaxed">100.00</field>
      <field name="date_invoice" eval="datetime.now().date()  - timedelta(days=20)"/>
      <field name="b2_file_id">1</field>
      <field name="move_id" ref="invoice_somconnexio.account_move_demo_bi" />
