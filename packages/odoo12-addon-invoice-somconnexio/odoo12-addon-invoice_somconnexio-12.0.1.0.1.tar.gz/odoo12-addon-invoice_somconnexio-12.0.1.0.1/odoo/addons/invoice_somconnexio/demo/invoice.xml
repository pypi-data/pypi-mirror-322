<?xml version="1.0" encoding="UTF-8"?>
<odoo>
   <record id="account_move_demo_oc" model="account.move">
      <field name="name">BILL/2022/0001</field>
      <field name="journal_id" ref="somconnexio.consumption_invoices_journal" />
   </record>
   <record id="account_move_demo_bi" model="account.move">
      <field name="name">BILL/2022/0002</field>
      <field name="journal_id" ref="invoice_somconnexio.customer_services_invoices_journal" />
   </record>
   <record id="account_type_demo" model="account.account.type">
      <field name="name">Bank and Cash</field>
      <field name="type">receivable</field>
   </record>
   <record id="account_demo" model="account.account">
      <field name="name">Liquidity Transfer</field>
      <field name="code">Test</field>
