<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="assets_frontend"
        name="Website Apps Store"
        inherit_id="website.assets_frontend"
    >
        <xpath expr="." position="inside">
            <link
                rel="stylesheet"
                type="text/scss"
                href="/website_apps_store/static/src/scss/main.scss"
            />
            <script
                type="text/javascript"
                src="/website_apps_store/static/src/js/website_sale.js"
            />
        </xpath>
    </template>
    <template id="assets_tests" inherit_id="website.assets_tests">
        <xpath expr=".">
            <script
                type="text/javascript"
                src="/website_apps_store/static/src/js/website_apps_store_tour.js"
            />
        </xpath>
    </template>
</odoo>
